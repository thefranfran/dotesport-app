{
  "expo": {
    "name": "dotEsport",
    "slug": "dotEsport",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.example.with-new-arch"
    },
    "android": {
      "package": "com.example.withnewarch"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-localization",
      "expo-font"
    ]
  }
}
